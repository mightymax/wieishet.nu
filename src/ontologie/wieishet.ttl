@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .

@prefix personen: <https://wieishet.nu/personen/> .
@prefix : <https://wieishet.nu/ontologie#> .

: a owl:Ontology ;
  rdfs:label "Wie is Het?"@nl ;
  dc:creator "Mark Lindeman <mark@lindeman.nu>" ;
  rdfs:comment "LoD versie van het aloude spel 'Wie is Het?'"@nl ;
.

:Persoon a owl:Class;
  rdfs:subClassOf schema:Persoon ;
  owl:equivalentClass [
    a owl:Restriction ;
    owl:onProperty schema:givenName ;
    owl:cardinality 1;
    rdfs:range xsd:string;
  ], [
    a owl:Restriction ;
    owl:onProperty schema:image ;
    owl:cardinality 1;
    rdfs:range rdf:resource;
  ];

  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :kleurhaar ;
    owl:allValuesFrom :KleurHaar ;
  ], [
    a owl:Restriction ;
    owl:onProperty :kleurogen ;
    owl:allValuesFrom :KleurOgen ;
  ], [
    a owl:Restriction ;
    owl:onProperty :geslacht ;
    owl:allValuesFrom :Geslacht ;
  ],[
    a owl:Restriction ;
    owl:onProperty :bril ;
    owl:oneOf ( true false );
  ],[
    a owl:Restriction ;
    owl:onProperty :hoofdbedekking ;
    owl:oneOf ( true false );
  ],[
    a owl:Restriction ;
    owl:onProperty :gezichtsbeharing ;
    owl:oneOf ( true false );
  ] ;
.

:KleurHaar a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :kleurhaar ;
    owl:allValuesFrom :Persoon ;
  ]
.

:KleurOgen a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :kleurhaar ;
    owl:allValuesFrom :Persoon ;
  ]
.

:Geslacht a owl:Class ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :geslacht ;
    owl:allValuesFrom :Persoon ;
  ]
.

# Data properties:
schema:givenName a owl:DatatypeProperty ;
  rdfs:domain :Persoon ;
  rdfs:range xsd:string ;
.

schema:image a owl:DatatypeProperty ;
  rdfs:domain :Persoon ;
  rdfs:range rdf:resource ;
.

:kleurhaar a owl:DatatypeProperty ;
  rdfs:domain :Persoon ;
  rdfs:range :KleurHaar
.

:kleurogen a owl:DatatypeProperty ;
  rdfs:domain :Persoon ;
  rdfs:range :KleurOgen
.

:geslacht a owl:DatatypeProperty ;
  rdfs:domain :Persoon ;
  rdfs:range :Geslacht ;
  owl:subPropertyOf schema:GenderType ; 
.

:bril a owl:DatatypeProperty ;
  rdfs:domain :Persoon ;
  rdfs:range xsd:boolean ;
. 

:hoofdbedekking a owl:DatatypeProperty ;
  rdfs:domain :Persoon ;
  rdfs:range xsd:boolean ;
. 

:gezichtsbeharing a owl:DatatypeProperty ;
  rdfs:domain :Persoon ;
  rdfs:range xsd:boolean ;
. 

:Blond a owl:NamedIndividual, :KleurHaar;
  rdfs:label "Blond" .

:Bruin a owl:NamedIndividual, :KleurHaar;
  rdfs:label "Bruin" .

:Rood a owl:NamedIndividual, :KleurHaar;
  rdfs:label "Rood" .

:Zwart a owl:NamedIndividual, :KleurHaar;
  rdfs:label "Zwart" .

:GrijsWit a owl:NamedIndividual, :KleurHaar;
  rdfs:label "Grijs/Wit" .

:Kaal a owl:NamedIndividual, :KleurHaar;
  rdfs:label "Kaal" .

:Geverfd a owl:NamedIndividual, :KleurHaar;
  rdfs:label "Geverfd/Gemixt" .

:Bruin a :KleurOgen .

:Blauw a owl:NamedIndividual, :KleurOgen;
  rdfs:label "Blauw" .

:Groen a owl:NamedIndividual, :KleurOgen;
  rdfs:label "Groen" .

:Man a owl:NamedIndividual, :Geslacht ;
  rdfs:label "Man" .

:Vrouw a owl:NamedIndividual, :Geslacht ;
  rdfs:label "Vrouw" .

:PersoonShape a sh:NodeShape ;
  sh:label "Persoon shape" ;
  sh:closed true;
  sh:ignoredProperties (rdf:type) ;
  sh:targetClass :Persoon ;
  sh:property [
    sh:path schema:givenName ;
    sh:minCount 1;
    sh:maxCount 1;
    sh:datatype xsd:string ;
  ], [
    sh:path :kleurhaar ;
    sh:minCount 1;
    sh:maxCount 1;
    sh:class :KleurHaar ;
  ], [
    sh:path :kleurogen ;
    sh:minCount 1;
    sh:maxCount 1;
    sh:class :KleurOgen ;
  ], [
    sh:path :geslacht ;
    sh:minCount 1;
    sh:maxCount 1;
    sh:class :Geslacht ;
  ], [
    sh:path :bril ;
    sh:minCount 1;
    sh:maxCount 1;
    sh:datatype xsd:boolean ;
  ], [
    sh:path :hoofdbedekking ;
    sh:minCount 1;
    sh:maxCount 1;
    sh:datatype xsd:boolean ;
  ], [
    sh:path :gezichtsbeharing ;
    sh:minCount 1;
    sh:maxCount 1;
    sh:datatype xsd:boolean ;
  ]
.


